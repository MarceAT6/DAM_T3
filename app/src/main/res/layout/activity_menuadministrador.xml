<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffb912"
    tools:context=".vistas.menuadministrador">

  <Button
      android:id="@+id/btn_cronologia"
      android:layout_width="350dp"
      android:layout_height="65dp"
      android:backgroundTint="#040303"
      android:text="CronologÃ­a "
      android:textColor="#ffb912"
      android:textSize="34sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_sedes"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.491"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_registro_curso"
      app:layout_constraintVertical_bias="0.433" />

  <ImageView
      android:id="@+id/imageView3"
      android:layout_width="376dp"
      android:layout_height="284dp"
      android:layout_marginBottom="180dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.542"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.37"
      app:srcCompat="@drawable/upn___universidad_privada_del_norte" />

  <Button
      android:id="@+id/btn_sedes"
      android:layout_width="350dp"
      android:layout_height="65dp"
      android:layout_marginBottom="16dp"
      android:backgroundTint="#000000"
      android:text="Sedes UPN"
      android:textColor="#ffb912"
      android:textSize="34sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.491"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView3"
      app:layout_constraintVertical_bias="0.928" />

  <TextView
      android:id="@+id/textView12"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Menu Administrador"
      android:textColor="#050505"
      android:textSize="36sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toTopOf="@+id/imageView3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.696" />

  <Button
      android:id="@+id/btn_registro_curso"
      android:layout_width="350dp"
      android:layout_height="65dp"
      android:backgroundTint="#040303"
      android:text="Registro Curso"
      android:textColor="#ffb912"
      android:textSize="34sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_sedes"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.491"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_registro_alumno"
      app:layout_constraintVertical_bias="0.105" />

  <Button
      android:id="@+id/btn_registro_alumno"
      android:layout_width="350dp"
      android:layout_height="65dp"
      android:layout_marginBottom="40dp"
      android:backgroundTint="#040303"
      android:text="Registro Alumno"
      android:textColor="#ffb912"
      android:textSize="34sp"
      app:layout_constraintBottom_toTopOf="@+id/btn_sedes"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.491"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView3"
      app:layout_constraintVertical_bias="0.096" />

</androidx.constraintlayout.widget.ConstraintLayout>